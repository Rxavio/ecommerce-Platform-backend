    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "globals": "^16.5.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.0.0",
